<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="serverListAnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.shellfire.vpn.gui.controller.ServerListSubviewController">
   <children>
            <TableView fx:id="serverListTableView" editable="true" fixedCellSize="0.0" prefHeight="650.0" prefWidth="400.0">
              <columns>
                <TableColumn fx:id="countryColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Country" />
                <TableColumn fx:id="nameColumn" maxWidth="190.0" minWidth="190.0" prefWidth="190.0" text="Name" />
                  <TableColumn fx:id="speedColumn" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="Speed" />
              </columns>
            </TableView>
   </children>
</AnchorPane>
