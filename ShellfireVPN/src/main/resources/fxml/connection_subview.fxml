      <ImageView fx:id="statusConnectionImageView" fitHeight="435.0" fitWidth="600.0">
         <image>
            <Image url="@../servers/australia-melbourne.jpg" />
         </image>
      </ImageView>
      <Button id="connect-button" fx:id="connectButton" layoutX="14.0" layoutY="378.0" mnemonicParsing="false" onAction="#handleConnectButtonAction" onMouseEntered="#handleConnectButtonEntered" onMouseExited="#handleConnectButtonExited" prefHeight="46.0" prefWidth="200.0" />
      <ImageView fx:id="connectImageView" fitHeight="46.0" fitWidth="200.0" layoutX="14.0" layoutY="378.0">
         <image>
            <Image url="@../buttons/button-connect-en.gif" />
