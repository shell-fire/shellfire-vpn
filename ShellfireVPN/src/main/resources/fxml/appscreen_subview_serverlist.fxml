<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="serverListAnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.shellfire.vpn.gui.controller.AppScreenControllerServerList">
   <children>
      <VBox>
         <children>
            <HBox prefHeight="30.0" prefWidth="420.0">
               <children>
                  <ImageView fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/search.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <TextField promptText="Type to filter..." HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
                  <TableView fx:id="serverListTableView" editable="true" fixedCellSize="0.0" prefHeight="640.0" prefWidth="420.0">
                    <columns>
                      <TableColumn fx:id="countryColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Country" />
                      <TableColumn fx:id="nameColumn" maxWidth="190.0" minWidth="190.0" prefWidth="190.0" text="Name" />
                        <TableColumn fx:id="speedColumn" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="Speed" />
                    </columns>
                  </TableView>
            <HBox prefHeight="50.0" prefWidth="420.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
